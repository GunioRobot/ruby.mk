.block
  .secondary-navigation
    %ul.wat-cf
      %li.first= link_to "#{t("web-app-theme.list", :default => "List")}", admin_editors_path
      %li.active= link_to "#{t("web-app-theme.new", :default => "New")}", new_admin_editor_path
  .content
    %h2.title
      = t("web-app-theme.new", :default => "New")
      Editor
    .inner
      = form_for :editor, :url => admin_editors_path, :html => { :class => :form } do |f|
        = render :partial => "form", :locals => {:f => f}
