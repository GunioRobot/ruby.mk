.block
  .secondary-navigation
    %ul.wat-cf
      %li.first= link_to "#{t("web-app-theme.list", :default => "List")}", admin_editors_path
      %li= link_to "#{t("web-app-theme.new", :default => "New")}", new_admin_editor_path
      %li.active= link_to "#{t("web-app-theme.edit", :default => "Edit")}", edit_admin_editor_path
  .content
    %h2.title
      = t("web-app-theme.edit", :default => "Edit")
      Editor
    .inner
      = form_for @editor, :url => admin_editor_path(@editor), :html => { :class => :form } do |f|
        = render :partial => "form", :locals => {:f => f}
